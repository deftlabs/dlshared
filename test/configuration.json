{
    "server": {

        "http": {
            "port": "9999",
            "bindAddress": "127.0.0.1"
        }
    },

	"version": "0.0.0",

	"environment": "local",

	"pidFile": "/tmp/dlshared_test.pid",

	"mongoDb": {
		"testDb": {
			"mongoUrl": "mongodb://localhost:28000/test",
			"mode": "strong",
			"type": "standalone",
			"dialTimeoutInMs": 30000,
			"socketTimeoutInMs": 30000,
			"syncTimeoutInMs": 30000,
			"cursorTimeoutInMs": 30000
		}
	},

	"cron": {
		"scheduled": {
			"mongoComponentId": "MongoTestDb",

			"definitionDbName": "test",
			"definitionCollectionName": "cron.definitions",

			"auditDbName": "test",
			"auditCollectionName": "cron.audit",
			"auditTimeoutInSec": 126144000,

			"distributedLockComponentId": "DistributedLock",

			"scheduledFunctions": [
				{ "jobId": "testCronJob-Run", "componentId": "testComponentId", "functionName": "Run", "schedule": "0 30 * * * *", "requiresDistributedLock": true, "audit": true },
				{ "jobId": "testCronJob-Test", "componentId": "testComponentId", "functionName": "Test", "schedule": "0 30 * * * *", "requiresDistributedLock": true, "audit": true }
			]
		}
	}
}
